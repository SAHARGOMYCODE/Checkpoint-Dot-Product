ALGORITHM Dot_product
 VAR
     
    i,j:integer;
    vect_1[i] : ARRAY_OF integer[i];
    vect_2[i] : ARRAY_OF integer[i];
    ps,n:  Integer;

BEGIN
 FUNCTION dot_product(vect_1[], int vect_2[]) : integer
   ps := 0;
   for ( i = 0; i < vect_1.length; i++)
   ps := ps + vect_1[i] * vect_2[i];
   return dot_product;
 END 
